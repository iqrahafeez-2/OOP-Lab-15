<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo1.ProductController">
    <TableView fx:id="tableView" layoutX="20.0" layoutY="20.0" prefHeight="200.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="100.0" text="ID"/>
            <TableColumn fx:id="nameColumn" prefWidth="150.0" text="Name"/>
            <TableColumn fx:id="priceColumn" prefWidth="150.0" text="Price"/>
        </columns>
    </TableView>
</AnchorPane>
